{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Service Catalog Tag options",
    "Resources": {
      "productEnvironmentProd" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "PROD",
          "Key" : "serviceEnvironment"
        }
      },
      "productEnvironmentDev" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "DEV",
          "Key" : "serviceEnvironment"
        }
      },
      "ChargeBackDataInsights" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "DATA-INSIGHTS",
          "Key" : "Chargeback"
        }
      },
      "ChargeBackDBs" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "DATABASES",
          "Key" : "Chargeback"
        }
      }
      ,
      "ChargeBackAccounting" : {
        "Type" : "AWS::ServiceCatalog::TagOption",
        "Properties" : {
          "Active" : true,
          "Value" : "ACCOUNTING",
          "Key" : "Chargeback"
        }
      }
    },
    "Outputs": {
    "TagEnvironmentProd": {
      "Value": {
        "Ref":"productEnvironmentProd"
      }
    },
    "TagEnvironmentDev": {
      "Value": {
        "Ref":"productEnvironmentDev"
      }
    },
    "TagChargeBackDataInsights": {
      "Value": {
        "Ref":"ChargeBackDataInsights"
      }
    },
    "TagChargeBackDBs": {
      "Value": {
        "Ref":"ChargeBackDBs"
      }
    },
    "TagChargeBackAcc": {
      "Value": {
        "Ref":"ChargeBackAccounting"
      }
    }
            
    }
}
